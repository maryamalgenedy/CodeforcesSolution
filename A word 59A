#include <iostream>
#include <cctype>
#include <cstring>
#include <cmath>
using namespace std;
int main(){
    
    int numOfUpper=0,numOfLower=0;
    string s;
    cin>>s;
    for(int i=0; i<s.length();i++){
        if(isupper(s[i])){
            numOfUpper++;
        }
        else{
            numOfLower++;
        }
    }
    if (numOfUpper>numOfLower){
        for (int i=0;i<s.length();i++){
            cout<<char(toupper(s[i]));
        }
    }
    else {
        for (int i=0;i<s.length();i++){
            cout<<char(tolower(s[i]));
    }
    }
}
