#include <iostream>
#include <cctype>
#include <cstring>
#include <cmath>
using namespace std;
int main(){
    string s;
    string s2="";
    cin>>s;
    int total=0;

    for (int i=0;i<s.length();i++){
        if (s2.find(s[i]) != string::npos ){
            continue;
        }
        else{
            total+=1;
            s2+=s[i];
        }
    }
    
    if (total%2==0){
        cout<<"CHAT WITH HER!"<<endl;
    }
    else cout<<"IGNORE HIM!"<<endl;
   
}



