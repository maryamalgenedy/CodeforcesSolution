#include <iostream>
#include <cctype>
#include <cstring>
#include <cmath>
using namespace std;
int main(){
    
    int n=1;
    string s;
    cin>>s;
 
    for (int i=1; i<s.length() ;i++){
        if (s[i]==s[i-1]){
            n++;
        }
        else {
            n=1;
        }
        if (n>=7){
            cout<<"YES"<<endl;
            return 0;
        }
    }
    cout<<"NO"<<endl;
    return 0;
}
