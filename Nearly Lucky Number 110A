#include <iostream>
using namespace std;
int main(){
    
    long long n;
    int total=0;
    cin>>n;
    while (n>0){
        if (n%10==4 || n%10==7){
            total++;
        }
        n=n/10;
    }
    bool c=false;
    
    while (total>0){
        if (total%10==4 || total%10==7){
            total/=10;
            c=true;
        }
        else{
            c=false;
            break;
        }
    }
    if (c==true){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
   
}
